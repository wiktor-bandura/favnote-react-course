{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:react/jsx-runtime",
      "prettier"
    ],
    "env": {
      "jest": true
    },
    "globals": {
      "document": true
    },
    "rules": {
       "react/react-in-jsx-scope": "off",
       "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
      }
}